{
  "name": "podopolo-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node -r @swc-node/register src/index.ts",
    "dev": "nodemon --ext ts --watch src --exec \"node -r @swc-node/register src/index.ts\"",
    "test": "mocha --require @swc-node/register 'tests/**/*.ts' --timeout 10000",
    "type-check": "tsc --noEmit",
    "docs": "typedoc",
    "clean": "rimraf dist",
    "build": "swc src -d dist",
    "build:prod": "NODE_ENV=production swc src -d dist",
    "build:type": "tsc --build",
    "prebuild": "npm run clean && npm run type-check",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
